{
  "rules": [
    {
      "pattern": "avue-crud",
      "replacement": "yl-crud"
    },
    {
      "pattern": "base-input-tag",
      "replacement": "yl-input-tag"
    },
    {
      "pattern": "import mixin from '@mixins/mixin'",
      "replacement": "import { mixin } from '@jt/yl-jms-mixins'"
    },
    {
      "pattern": "import { RESPONSE_CODE } from '@public/http/config'",
      "replacement": "const { RESPONSE_CODE } = $ylConfig.http"
    },
    {
      "pattern": "import fetch from '@public/http/request'",
      "replacement": "import fetch from '@jt/jms-common-jbs/@public/http/request'"
    },
    {
      "pattern": "import { lang } from '@public/utils/filters'",
      "replacement": "const { lang } = $ylI18n"
    },
    {
      "pattern": "avue-form",
      "replacement": "yl-form"
    },
    {
      "pattern": "import { RULES } from '@public/utils/rules'",
      "replacement": "const { RULES } = $ylConfig"
    },
    {
      "pattern": "import { interfaceWaitHandle } from '@public/utils/common'",
      "replacement": "const { interfaceWaitHandle } = $ylUtils.commonFun"
    },
    {
      "pattern": "@public/utils/filters",
      "replacement": "@jt/jms-common-jbs/@public/utils/filters"
    },
    {
      "pattern": "import { GATEWAY } from '@public/config/index'",
      "replacement": "const { GATEWAY } = $ylConfig.http"
    },
    {
      "pattern": "import { commonFun } from '@public/utils/common'",
      "replacement": "const { commonFun } = $ylUtils"
    }
  ]
}
